<form action="/register" method="post" id="registerform">
  First Name  <input id="first_name" type="text" name="first_name" placeholder="Timo"><br>
  Last Name  <input id="last_name" type="text" name="last_name" placeholder="Boll"><br>
  User Name  <input id="user_name" type="text" name="user_name" placeholder="Ping Pong Champ"><br>
  E-mail  <input id="email" type="text" name="email" placeholder="ping@pong.com" size = "28px"><br>
  Password  <input id="password" type="password" name = "password" placeholder="password"><br>
  <input id ="submit" type ="submit" value="Sign Up">
</form>
